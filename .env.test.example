# Test Environment Variables for E2E Tests
# Copy this file to .env.test and fill in your test credentials

# Bodhi Development Client ID (configure redirect URL: http://localhost:5173/my-test-app/)
TEST_DEV_CLIENT_ID=your-dev-client-id-here

# Bodhi Test Account Credentials (for Keycloak authentication)
TEST_BODHI_USERNAME=your-test-username
TEST_BODHI_PASSWORD=your-test-password
